generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           String   @id @default(uuid())
  email        String   @unique
  username     String   @unique
  name         String?
  password     String
  profilePhoto String?
  bio          String?
  DOB          String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  gender       gender?
}

enum gender {
  MALE
  FEMALE
}
